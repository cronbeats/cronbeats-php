{
  "name": "cronbeats/cronbeats-php",
  "description": "Official CronBeats Ping SDK for PHP.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "CronBeats\\PingSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CronBeats\\PingSdk\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
